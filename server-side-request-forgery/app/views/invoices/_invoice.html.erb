<div id="<%= dom_id invoice %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Issue date:</strong>
    <%= invoice.issue_date %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Url:</strong>
    <%= invoice.url %>
  </div>
  <div class="break-words">
    <strong class="block font-medium mb-1">Raw data:</strong>
    <%= invoice.raw_data %>
  </div>
  <div class="break-words">
    <strong class="block font-medium mb-1">Parsed data:</strong>
    <%= invoice.parsed_data %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Status:</strong>
    <%= invoice.status %>
  </div>
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= invoice.name %>
  </div>
</div>
